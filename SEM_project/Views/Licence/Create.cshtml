@model SEM_project.Models.Entities.Licence
@{
    ViewData["Title"] = "Nuevo";
}

<h3 style="color: #326091;"><b>Nuevo Licencia</b></h3>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    

            <div class="form-group">
                <label asp-for="LicenceName" class="control-label">LicenceName</label>
                <input asp-for="LicenceName" class="form-control" />
                <span asp-validation-for="LicenceName" class="text-danger"></span>
            </div>



            <div class="form-group">
                <label asp-for="Version" class="control-label">Version</label>
                <input asp-for="Version" class="form-control" />
                <span asp-validation-for="Version" class="text-danger"></span>
            </div>

                      
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" class="back-button">
        <span>&#8592;</span> Regresar
    </a>
</div>
